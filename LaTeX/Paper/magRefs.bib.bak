@Article{morrow2001,
  Title                    = {The Institutional Features of the Prisoners of War Treaties},
  Author                   = {Morrow, James D.},
  Journal                  = {International Organization},
  Year                     = {2001},
  Number                   = {4},
  Pages                    = {pp. 971-991},
  Volume                   = {55},

  Abstract                 = {During the twentieth century states negotiated and ratified formal treaties on the treatment of prisoners of war (POWs). These treaties have created a system for the treatment of POWs with universal and detailed standards and decentralized enforcement. I explain the form of the POW system as a rational institutional response to four strategic problems the issue of POWs poses: monitoring under noise, individual as opposed to state violations, variation in preferred treatment of POWs, and raising a mass army. In response to these four problems, neutral parties help address the problem of monitoring the standards. The ratification process screens out some states that do not intend to live up to the standards. The two-level problem of state and individual violations is addressed by making states responsible for punishing the actions of their own soldiers. By protecting POWs, the treaties help states raise armies during wartime. The POW case supports many, but not all, of the Rational Design conjectures. In particular, it suggests other strategic logics to explain variation in the membership and centralization of international institutions.},
  Copyright                = {Copyright © 2001 The MIT Press},
  ISSN                     = {00208183},
  Jstor_articletype        = {research-article},
